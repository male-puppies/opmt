config system
	option hostname EC700A
	option timezone CST-8
	option zonename Asia/Shanghai
	option cronloglevel 8
	option conloglevel 6

config timeserver ntp
	option enabled '1'
	list server 'ntp.api.bz'
	list server 's2b.time.edu.cn'
	list server 's2k.time.edu.cn'
	list server 's2m.time.edu.cn'
	list server 's1a.time.edu.cn'
